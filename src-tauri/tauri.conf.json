{
  "build": {
    "beforeDevCommand": "yarn dev:vite",
    "beforeBuildCommand": "yarn build:vite",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "feliongui",
    "version": "4.8.3"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "fs": {
        "all": true,
        "scope": [
          "**",
          "$APPLOCALDATA/**",
          "$RESOURCE/**"
        ]
      },
      "path": {
        "all": true
      },
      "shell": {
        "all": true,
        "sidecar": true,
        "scope": [
          {
            "name": "felionpy",
            "cmd": "$APPLOCALDATA/felionpy/felionpy",
            "args": true
          },
          {
            "name": "python",
            "cmd": "python",
            "args": true
          },
          {
            "name": "netstat-win32",
            "cmd": "netstat",
            "args": true
          },
          {
            "name": "netstat-darwin",
            "cmd": "lsof",
            "args": [
              "-i",
              {
                "validator": "\\S+"
              }
            ]
          },
          {
            "name": "taskkill-win32",
            "cmd": "taskkill",
            "args": [
              "/PID",
              {
                "validator": "\\S+"
              },
              "/F"
            ]
          },
          {
            "name": "taskkill-darwin",
            "cmd": "kill",
            "args": [
              "-9",
              {
                "validator": "\\S+"
              }
            ]
          },
          {
            "name": "unzip-win32",
            "cmd": "tar",
            "args": [
              "-xf",
              {
                "validator": "\\S+"
              },
              "-C",
              {
                "validator": "\\S+"
              }
            ]
          },
          {
            "name": "unzip-darwin",
            "cmd": "unzip",
            "args": [
              {
                "validator": "\\S+"
              },
              "-d",
              {
                "validator": "\\S+"
              }
            ]
          }
        ]
      },
      "dialog": {
        "all": true
      },
      "process": {
        "all": false,
        "exit": false,
        "relaunch": true,
        "relaunchDangerousAllowSymlinkMacos": false
      },
      "os": {
        "all": true
      },
      "window": {
        "all": false,
        "close": true
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.felion.app",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [
        "resources/**/*"
      ],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null,
      "devCsp": null
    },
    "updater": {
      "active": true,
      "endpoints": [
        "https://felion-update-server.aravindhnivas.workers.dev/v1/{{target}}/{{arch}}/{{current_version}}"
      ],
      "dialog": false,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEJERkFCNEYzNDExRjYyRTIKUldUaVloOUI4N1Q2dmVqZDVwUEVjYytMa1VUZEVlUVhsN1ZLdlFISjM4Wkp5Y1pYcGtVdHd2cGwK",
      "windows": {
        "installMode": "passive"
      }
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 700,
        "resizable": true,
        "title": "FELion v4",
        "width": 1200
      }
    ]
  }
}